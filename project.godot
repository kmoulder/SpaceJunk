config_version=5

[application]

config/name="Space Factory"
config/description="A Factorio-inspired automation game set on a space station drifting through a debris field."
run/main_scene="res://scenes/game/Main.tscn"
config/features=PackedStringArray("4.5", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/csharp/GameManager.cs"
GridManager="*res://scripts/csharp/GridManager.cs"
SpriteGenerator="*res://scripts/csharp/SpriteGenerator.cs"
InventoryManager="*res://scripts/csharp/InventoryManager.cs"
CraftingManager="*res://scripts/csharp/CraftingManager.cs"
DebrisManager="*res://scripts/csharp/DebrisManager.cs"
ResearchManager="*res://scripts/csharp/ResearchManager.cs"
PowerManager="*res://scripts/csharp/PowerManager.cs"
BuildingManager="*res://scripts/csharp/BuildingManager.cs"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="Space Factory"

